net: "COLOR/finetune_alexnet_model.prototxt"

# test config
test_iter: 100
test_interval: 1000
test_initialization: true

# progress & policy config
base_lr: 0.00001
lr_policy: "multistep"
stepvalue:15000
stepvalue:21000
stepvalue:26000
max_iter: 30000
momentum: 0.9
weight_decay: 0.0002
gamma: 0.1
average_loss: 100

# display config
display: 100
snapshot: 1000
snapshot_prefix: "COLOR/color_alexnet_f48"

# solver config
solver_mode: GPU
device_id:0
